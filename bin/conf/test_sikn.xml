				<variable convert="float">io.globaltemp.current.value</variable>
				<variable convert="float">io.globaltemp.keypad</variable>
				<variable                >io.globaltemp.scales.min</variable>
				<variable                >io.globaltemp.scales.max</variable>
				<variable                >io.globaltemp.status</variable>
				<variable                >io.globaltemp.mode</variable>
